<!DOCTYPE html>
<html>
<head>
    <link href="~/Content/bootstrap.css" rel="
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Dashboard</title>
</head>
<body>
    <div class="row">
        <div class="col-lg-2-md-2 col-sm-2 col-xs-2" style="height:900px;border:1px solid gray; background-color:dimgrey">

            <div class="list-group">
                <div class="list-group-item">
                    @Html.ActionLink("Add_Category", "Add_Category", new { @class = "list-group-item" })
                </div>
                <div class="list-group-item">
                    @Html.ActionLink("AddQuestions", "AddQuestions", new { @class = "list-group-item" })
                </div>
                <div class="list-group-item">
                    @Html.ActionLink("Reports", "Dashboard", new { @class = "list-group-item" })
                </div>
                <br />
                <div class="list-group-item">
                    @Html.ActionLink("Logout", "Logout", new { @class = "list-group-item" })
                </div>
            </div>
        </div>

        <div class="col-lg-10-md-10 col-sm-10 col-xs-10" style="height:900px;border:1px solid gray">
            @RenderBody()
        </div>
    </div>



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</body>
</html>
